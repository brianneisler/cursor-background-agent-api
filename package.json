{
  "name": "cursor-api-client",
  "version": "1.0.1",
  "description": "Simple API client for Cursor Background Composer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js"
    }
  },
  "bin": {
    "cursor-api": "./dist/cli.js"
  },
  "files": [
    "src/**/*",
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "mcp-server": "tsx src/cli.ts mcp-server",
    "mcp-server:dev": "tsx src/cli.ts mcp-server --verbose",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.15.0",
    "axios": "^1.6.0",
    "axios-retry": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express": "^5.0.1",
    "pino": "^8.17.0",
    "pino-pretty": "^10.3.0",
    "yargs": "^17.7.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^20.10.0",
    "@types/yargs": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "cursor",
    "api",
    "background-composer",
    "typescript",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjdierkes/cursor-background-agent-api.git"
  },
  "bugs": {
    "url": "https://github.com/mjdierkes/cursor-background-agent-api/issues"
  },
  "homepage": "https://github.com/mjdierkes/cursor-background-agent-api#readme"
}